export APORETO_BUILD_CONTAINER_IMAGE="gcr.io/aporetodev/domingo-alpine:v3.58.0"

alias ap.buildc='f() {
   [[ $GOPATH ]] || { echo "GOPATH is not set"; return 2; }
   docker run -it -v $GOPATH/src:/go/src $APORETO_BUILD_CONTAINER_IMAGE
};f'

alias git.info='f() {
    [ -f ~/.gittoken ] && { echo "jody@aporeto.com"; cat ~/.gittoken; } || { echo "missing gittoken"; return 2; }
    return 0
};f'
