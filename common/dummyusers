function getpass() {
   user=$1
   [[ $user ]] || { err "Usage:getpass user"; return 2; }
   [ -d ${HOME}/.dummys/${user} ] || { err "User $user not found"; return 3; }
   [ -f ${HOME}/.dummys/${user}/pass ] || { err "User exist but does not have password set"; return 3; }
   cat ${HOME}/.dummys/${user}/pass
   return 0
}

function loadpass() {
    getpass $@ | pbcopy
}
